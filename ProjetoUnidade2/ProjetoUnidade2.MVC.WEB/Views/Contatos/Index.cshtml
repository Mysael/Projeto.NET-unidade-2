@model IEnumerable<ProjetoUnidade2.MVC.WEB.Models.Contato>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telefone)
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Nome)
                </td>
                <td>
                    <!--//Html.DisplayFor(modelItem => item.Telefone.Numero)-->
                    @Html.DisplayFor(modelItem => item.Telefone.Numero)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.ContatoID }) |
                    @Html.ActionLink("Details", "Details", new { id = item.ContatoID }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.ContatoID })
                </td>
            </tr>
        }

    </table>
</body>
</html>
